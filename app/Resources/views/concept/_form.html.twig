{{ form_start(form, {'attr': {'class': 'container-fluid form-group',
						 'novalidate': 'novalidate'
						} 
}) }}
{{ form_errors(form) }}
    <div class="form-row align-items-center mb-1"> 
	<div class="col-lg-2 col-md-2 col-sm-6">
{#		{{ form_label(form.numeroReferencia, null, { 'label_attr': {'class':'col-form-label-sm' } }) }}#}
	    {{ form_label(form.entity) }}
	    {{ form_widget(form.entity, { 'attr': { 'class' : 'form-control-sm' } }) }}
	    {{ form_errors(form.entity) }}
	</div>
	<div class="col-lg-2 col-md-2 col-sm-6">
{#		{{ form_label(form.numeroReferencia, null, { 'label_attr': {'class':'col-form-label-sm' } }) }}#}
	    {{ form_label(form.suffix) }}
	    {{ form_widget(form.suffix, { 'attr': { 'class' : 'form-control-sm' } }) }}
	    {{ form_errors(form.suffix) }}
	</div>
	<div class="col-lg-2 col-md-2 col-sm-6">
{#		{{ form_label(form.numeroReferencia, null, { 'label_attr': {'class':'col-form-label-sm' } }) }}#}
	    {{ form_label(form.name) }}
	    {{ form_widget(form.name, { 'id': 'input-numeroReferencia', 'attr': { 'class' : 'form-control-sm' } }) }}
	    {{ form_errors(form.name) }}
	</div>
	<div class="col-lg-2 col-md-2 col-sm-6">
	    {{ form_label(form.unitaryPrice) }}
	    {{ form_widget(form.unitaryPrice, { 'attr': { 'id': 'input-dni', 'class' : 'form-control-sm' } }) }}
	    {{ form_errors(form.unitaryPrice) }}
	</div>
    </div>
    {% if readonly == false %}
	{{ form_widget(form.save, { 'attr': { 'class' : 'btn btn-primary ml-1' } }) }}
    {% else %}
	{% set url = path('concept_list') %}
	{{ form_widget(form.back, { 'attr': { 'class' : 'js-back btn btn-primary ml-1' ,
					     'data-url' : url },
				  }) }}
    {% endif %}
    </div>
{{ form_end(form, {'attr': {'class': '',
			       } 
}) }}
